// function calling 


import 'dart:io';

void main() {
  print('Welcome to dart ');

 var myC= myClass();
 myC.printName("Ws Cube Tech");  // Function Calling
  myC.printName("Ana de Armas");
  myC.printName("Galib MAhmud");


}
class myClass{

  void printName(String name){

    print(name);
  }
}

import 'dart:io';

void main() {
  print('Welcome to dart ');

 var myC= myClass();
 myC.printName("Ws Cube Tech");  // Function Calling
  myC.printName("Ana de Armas");
  myC.printName("Galib MAhmud");

 print ("Sum is : ${myC.Add()}") ;


}
class myClass{

  void printName(String name){

    print(name);
  }
  int Add(){
    int a=5,b=6;
    int sum =a+b;
    return sum;

  }
}


